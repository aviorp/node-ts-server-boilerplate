generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "darwin"]
}

datasource db {
  provider = "mongodb"
  url      = env("DB_URI")
}

model User {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  first_name String
  last_name  String
  password   String
  image      String
  username   String
  is_admin   Boolean? @default(false)
  created_at String?
}
