


// -------------------------------------- //
//              User Model                //
// -------------------------------------- //

model User {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  first_name String
  last_name  String
  password   String
  image      String
  username   String   @unique
  is_admin   Boolean? @default(false)
  created_at String?
}

// -------------------------------------- //
//              User Model End            //
// -------------------------------------- //




